{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#191c2490",
                "editor.background": "#191c2490",
                "editor.foreground": "#c5c6c8",
                "text": "#c5c6c8",
                "text.muted": "#c5c6c870",
                "text.ignored": "#c5c6c840",
                "text.placeholder": "#c5c6c850",
                "ignored": "#c5c6c830",
                "element.hover": "#c5c6c830",
                "ghost_element.hover": "#3E5A8130",
                "ghost_element.selected": "#3E5A8130",
                "ghost_element.active": "#3E5A8160",
                "border": "#191c24",
                "editor.highlighted_line.background": "#3E5A8110",
                "editor.active_line.background": "#3E5A8110",
                "panel.background": "#191c2490",
                "title_bar.background": "#191c2490",
                "title_bar.inactive_background": "#191c2490",
                "status_bar.background": "#191c2490",
                "drop_target.background": "#191c2490",
                "elevated_surface.background": "#191c24",
                "toolbar.background": "#191c2490",
                "tab_bar.background": "#191c2490",
                "tab.inactive_background": "#191c2490",
                "tab.active_background": "#3E5A8130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#191c24",
                "scrollbar.thumb.background": "#3E5A81",
                "editor.gutter.background": "#191c2490",
                "terminal.background": "#191c2410",
                "terminal.foreground": "#c5c6c8",
                "terminal.dim_foreground": "#c5c6c8",
                "terminal.bright_foreground": "#c5c6c8",
                "terminal.ansi.black": "#191c24",
                "terminal.ansi.red": "#3E5A81",
                "terminal.ansi.green": "#5B7192",
                "terminal.ansi.yellow": "#6C8DAE",
                "terminal.ansi.blue": "#7AABC4",
                "terminal.ansi.magenta": "#B08482",
                "terminal.ansi.cyan": "#9B96AC",
                "terminal.ansi.white": "#c5c6c8",
                "terminal.ansi.bright_black": "#616777",
                "terminal.ansi.bright_red": "#3E5A81",
                "terminal.ansi.bright_green": "#5B7192",
                "terminal.ansi.bright_yellow": "#6C8DAE",
                "terminal.ansi.bright_blue": "#7AABC4",
                "terminal.ansi.bright_magenta": "#B08482",
                "terminal.ansi.bright_cyan": "#9B96AC",
                "terminal.ansi.bright_white": "#c5c6c8",
                "modified": "#3E5A81",
                "syntax": {
                    "attribute": {
                        "color": "#3E5A81"
                    },
                    "boolean": {
                      "color": "#5B7192"
                    },
                    "comment": {
                      "color": "#616777"
                    },
                    "comment.doc": {
                      "color": "#616777"
                    },
                    "constant": {
                      "color": "#5B7192"
                    },
                    "function": {
                      "color": "#9B96AC"
                    },
                    "keyword": {
                      "color": "#7AABC4"
                    },
                    "number": {
                      "color": "#B08482"
                    },
                    "operator": {
                      "color": "#7AABC4"
                    },
                    "string": {
                      "color": "#3E5A81"
                    },
                    "variable": {
                      "color": "#5B7192"
                    }
                },
                "players": [
                  {
                    "cursor": "#3E5A81",
                    "background": "#191c24",
                    "selection": "#3E5A8130"
                  }
                ]
            }
        }
    ]
}
