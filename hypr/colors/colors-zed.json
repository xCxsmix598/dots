{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#12171f90",
                "editor.background": "#12171f90",
                "editor.foreground": "#c3c5c7",
                "text": "#c3c5c7",
                "text.muted": "#c3c5c770",
                "text.ignored": "#c3c5c740",
                "text.placeholder": "#c3c5c750",
                "ignored": "#c3c5c730",
                "element.hover": "#c3c5c730",
                "ghost_element.hover": "#6E91AF30",
                "ghost_element.selected": "#6E91AF30",
                "ghost_element.active": "#6E91AF60",
                "border": "#12171f",
                "editor.highlighted_line.background": "#6E91AF10",
                "editor.active_line.background": "#6E91AF10",
                "panel.background": "#12171f90",
                "title_bar.background": "#12171f90",
                "title_bar.inactive_background": "#12171f90",
                "status_bar.background": "#12171f90",
                "drop_target.background": "#12171f90",
                "elevated_surface.background": "#12171f",
                "toolbar.background": "#12171f90",
                "tab_bar.background": "#12171f90",
                "tab.inactive_background": "#12171f90",
                "tab.active_background": "#6E91AF30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#12171f",
                "scrollbar.thumb.background": "#6E91AF",
                "editor.gutter.background": "#12171f90",
                "terminal.background": "#12171f10",
                "terminal.foreground": "#c3c5c7",
                "terminal.dim_foreground": "#c3c5c7",
                "terminal.bright_foreground": "#c3c5c7",
                "terminal.ansi.black": "#12171f",
                "terminal.ansi.red": "#6E91AF",
                "terminal.ansi.green": "#7496AC",
                "terminal.ansi.yellow": "#7CA8C2",
                "terminal.ansi.blue": "#8DA2B4",
                "terminal.ansi.magenta": "#9AB5C7",
                "terminal.ansi.cyan": "#B4C7D5",
                "terminal.ansi.white": "#c3c5c7",
                "terminal.ansi.bright_black": "#5e6772",
                "terminal.ansi.bright_red": "#6E91AF",
                "terminal.ansi.bright_green": "#7496AC",
                "terminal.ansi.bright_yellow": "#7CA8C2",
                "terminal.ansi.bright_blue": "#8DA2B4",
                "terminal.ansi.bright_magenta": "#9AB5C7",
                "terminal.ansi.bright_cyan": "#B4C7D5",
                "terminal.ansi.bright_white": "#c3c5c7",
                "modified": "#6E91AF",
                "syntax": {
                    "attribute": {
                        "color": "#6E91AF"
                    },
                    "boolean": {
                      "color": "#7496AC"
                    },
                    "comment": {
                      "color": "#5e6772"
                    },
                    "comment.doc": {
                      "color": "#5e6772"
                    },
                    "constant": {
                      "color": "#7496AC"
                    },
                    "function": {
                      "color": "#B4C7D5"
                    },
                    "keyword": {
                      "color": "#8DA2B4"
                    },
                    "number": {
                      "color": "#9AB5C7"
                    },
                    "operator": {
                      "color": "#8DA2B4"
                    },
                    "string": {
                      "color": "#6E91AF"
                    },
                    "variable": {
                      "color": "#7496AC"
                    }
                },
                "players": [
                  {
                    "cursor": "#6E91AF",
                    "background": "#12171f",
                    "selection": "#6E91AF30"
                  }
                ]
            }
        }
    ]
}
