{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#181b2290",
                "editor.background": "#181b2290",
                "editor.foreground": "#c5c6c7",
                "text": "#c5c6c7",
                "text.muted": "#c5c6c770",
                "text.ignored": "#c5c6c740",
                "text.placeholder": "#c5c6c750",
                "ignored": "#c5c6c730",
                "element.hover": "#c5c6c730",
                "ghost_element.hover": "#68787E30",
                "ghost_element.selected": "#68787E30",
                "ghost_element.active": "#68787E60",
                "border": "#181b22",
                "editor.highlighted_line.background": "#68787E10",
                "editor.active_line.background": "#68787E10",
                "panel.background": "#181b2290",
                "title_bar.background": "#181b2290",
                "title_bar.inactive_background": "#181b2290",
                "status_bar.background": "#181b2290",
                "drop_target.background": "#181b2290",
                "elevated_surface.background": "#181b22",
                "toolbar.background": "#181b2290",
                "tab_bar.background": "#181b2290",
                "tab.inactive_background": "#181b2290",
                "tab.active_background": "#68787E30",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#181b22",
                "scrollbar.thumb.background": "#68787E",
                "editor.gutter.background": "#181b2290",
                "terminal.background": "#181b2210",
                "terminal.foreground": "#c5c6c7",
                "terminal.dim_foreground": "#c5c6c7",
                "terminal.bright_foreground": "#c5c6c7",
                "terminal.ansi.black": "#181b22",
                "terminal.ansi.red": "#68787E",
                "terminal.ansi.green": "#537E82",
                "terminal.ansi.yellow": "#767D83",
                "terminal.ansi.blue": "#7D8288",
                "terminal.ansi.magenta": "#85898E",
                "terminal.ansi.cyan": "#9A9EA2",
                "terminal.ansi.white": "#c5c6c7",
                "terminal.ansi.bright_black": "#606676",
                "terminal.ansi.bright_red": "#68787E",
                "terminal.ansi.bright_green": "#537E82",
                "terminal.ansi.bright_yellow": "#767D83",
                "terminal.ansi.bright_blue": "#7D8288",
                "terminal.ansi.bright_magenta": "#85898E",
                "terminal.ansi.bright_cyan": "#9A9EA2",
                "terminal.ansi.bright_white": "#c5c6c7",
                "modified": "#68787E",
                "syntax": {
                    "attribute": {
                        "color": "#68787E"
                    },
                    "boolean": {
                      "color": "#537E82"
                    },
                    "comment": {
                      "color": "#606676"
                    },
                    "comment.doc": {
                      "color": "#606676"
                    },
                    "constant": {
                      "color": "#537E82"
                    },
                    "function": {
                      "color": "#9A9EA2"
                    },
                    "keyword": {
                      "color": "#7D8288"
                    },
                    "number": {
                      "color": "#85898E"
                    },
                    "operator": {
                      "color": "#7D8288"
                    },
                    "string": {
                      "color": "#68787E"
                    },
                    "variable": {
                      "color": "#537E82"
                    }
                },
                "players": [
                  {
                    "cursor": "#68787E",
                    "background": "#181b22",
                    "selection": "#68787E30"
                  }
                ]
            }
        }
    ]
}
