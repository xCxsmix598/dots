{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#1b1e2590",
                "editor.background": "#1b1e2590",
                "editor.foreground": "#c6c6c8",
                "text": "#c6c6c8",
                "text.muted": "#c6c6c870",
                "text.ignored": "#c6c6c840",
                "text.placeholder": "#c6c6c850",
                "ignored": "#c6c6c830",
                "element.hover": "#c6c6c830",
                "ghost_element.hover": "#D0897130",
                "ghost_element.selected": "#D0897130",
                "ghost_element.active": "#D0897160",
                "border": "#1b1e25",
                "editor.highlighted_line.background": "#D0897110",
                "editor.active_line.background": "#D0897110",
                "panel.background": "#1b1e2590",
                "title_bar.background": "#1b1e2590",
                "title_bar.inactive_background": "#1b1e2590",
                "status_bar.background": "#1b1e2590",
                "drop_target.background": "#1b1e2590",
                "elevated_surface.background": "#1b1e25",
                "toolbar.background": "#1b1e2590",
                "tab_bar.background": "#1b1e2590",
                "tab.inactive_background": "#1b1e2590",
                "tab.active_background": "#D0897130",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#1b1e25",
                "scrollbar.thumb.background": "#D08971",
                "editor.gutter.background": "#1b1e2590",
                "terminal.background": "#1b1e2510",
                "terminal.foreground": "#c6c6c8",
                "terminal.dim_foreground": "#c6c6c8",
                "terminal.bright_foreground": "#c6c6c8",
                "terminal.ansi.black": "#1b1e25",
                "terminal.ansi.red": "#D08971",
                "terminal.ansi.green": "#5B769A",
                "terminal.ansi.yellow": "#967789",
                "terminal.ansi.blue": "#5E81AB",
                "terminal.ansi.magenta": "#6C8BAD",
                "terminal.ansi.cyan": "#7CA2C2",
                "terminal.ansi.white": "#c6c6c8",
                "terminal.ansi.bright_black": "#626878",
                "terminal.ansi.bright_red": "#D08971",
                "terminal.ansi.bright_green": "#5B769A",
                "terminal.ansi.bright_yellow": "#967789",
                "terminal.ansi.bright_blue": "#5E81AB",
                "terminal.ansi.bright_magenta": "#6C8BAD",
                "terminal.ansi.bright_cyan": "#7CA2C2",
                "terminal.ansi.bright_white": "#c6c6c8",
                "modified": "#D08971",
                "syntax": {
                    "attribute": {
                        "color": "#D08971"
                    },
                    "boolean": {
                      "color": "#5B769A"
                    },
                    "comment": {
                      "color": "#626878"
                    },
                    "comment.doc": {
                      "color": "#626878"
                    },
                    "constant": {
                      "color": "#5B769A"
                    },
                    "function": {
                      "color": "#7CA2C2"
                    },
                    "keyword": {
                      "color": "#5E81AB"
                    },
                    "number": {
                      "color": "#6C8BAD"
                    },
                    "operator": {
                      "color": "#5E81AB"
                    },
                    "string": {
                      "color": "#D08971"
                    },
                    "variable": {
                      "color": "#5B769A"
                    }
                },
                "players": [
                  {
                    "cursor": "#D08971",
                    "background": "#1b1e25",
                    "selection": "#D0897130"
                  }
                ]
            }
        }
    ]
}
