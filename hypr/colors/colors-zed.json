{
    "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
    "name": "pywal",
    "author": "pywal",
    "themes": [
        {
            "name": "pywal",
            "appearance": "dark",
            "style": {
                "background": "#1b1e2590",
                "editor.background": "#1b1e2590",
                "editor.foreground": "#c6c6c8",
                "text": "#c6c6c8",
                "text.muted": "#c6c6c870",
                "text.ignored": "#c6c6c840",
                "text.placeholder": "#c6c6c850",
                "ignored": "#c6c6c830",
                "element.hover": "#c6c6c830",
                "ghost_element.hover": "#65818530",
                "ghost_element.selected": "#65818530",
                "ghost_element.active": "#65818560",
                "border": "#1b1e25",
                "editor.highlighted_line.background": "#65818510",
                "editor.active_line.background": "#65818510",
                "panel.background": "#1b1e2590",
                "title_bar.background": "#1b1e2590",
                "title_bar.inactive_background": "#1b1e2590",
                "status_bar.background": "#1b1e2590",
                "drop_target.background": "#1b1e2590",
                "elevated_surface.background": "#1b1e25",
                "toolbar.background": "#1b1e2590",
                "tab_bar.background": "#1b1e2590",
                "tab.inactive_background": "#1b1e2590",
                "tab.active_background": "#65818530",
                "scrollbar.track.background": "transparent",
                "scrollbar.track.border": "#1b1e25",
                "scrollbar.thumb.background": "#658185",
                "editor.gutter.background": "#1b1e2590",
                "terminal.background": "#1b1e2510",
                "terminal.foreground": "#c6c6c8",
                "terminal.dim_foreground": "#c6c6c8",
                "terminal.bright_foreground": "#c6c6c8",
                "terminal.ansi.black": "#1b1e25",
                "terminal.ansi.red": "#658185",
                "terminal.ansi.green": "#688589",
                "terminal.ansi.yellow": "#6E8E91",
                "terminal.ansi.blue": "#6F8E91",
                "terminal.ansi.magenta": "#7B9FA0",
                "terminal.ansi.cyan": "#7DA2A4",
                "terminal.ansi.white": "#c6c6c8",
                "terminal.ansi.bright_black": "#626878",
                "terminal.ansi.bright_red": "#658185",
                "terminal.ansi.bright_green": "#688589",
                "terminal.ansi.bright_yellow": "#6E8E91",
                "terminal.ansi.bright_blue": "#6F8E91",
                "terminal.ansi.bright_magenta": "#7B9FA0",
                "terminal.ansi.bright_cyan": "#7DA2A4",
                "terminal.ansi.bright_white": "#c6c6c8",
                "modified": "#658185",
                "syntax": {
                    "attribute": {
                        "color": "#658185"
                    },
                    "boolean": {
                      "color": "#688589"
                    },
                    "comment": {
                      "color": "#626878"
                    },
                    "comment.doc": {
                      "color": "#626878"
                    },
                    "constant": {
                      "color": "#688589"
                    },
                    "function": {
                      "color": "#7DA2A4"
                    },
                    "keyword": {
                      "color": "#6F8E91"
                    },
                    "number": {
                      "color": "#7B9FA0"
                    },
                    "operator": {
                      "color": "#6F8E91"
                    },
                    "string": {
                      "color": "#658185"
                    },
                    "variable": {
                      "color": "#688589"
                    }
                },
                "players": [
                  {
                    "cursor": "#658185",
                    "background": "#1b1e25",
                    "selection": "#65818530"
                  }
                ]
            }
        }
    ]
}
